@prefix lv2:  <http://lv2plug.in/ns/lv2core#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix doap: <http://usefulinc.com/ns/doap#> .

<http://fps.io/plugins/stereofication> a lv2:Plugin ;
  doap:name "stereofication";
  doap:license <http://usefulinc.com/doap/licenses/gpl> ;
  doap:description "Stochastic timestretching one channel to reduce correlation between left and right" ;
  lv2:binary <stereo.so> ;
  lv2:optionalFeature lv2:hardRTCapable ;
  lv2:port [
    a lv2:AudioPort ;
    a lv2:InputPort ;
    lv2:index 0 ;
    lv2:symbol "in" ;
    lv2:name "In"
  ] , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 1 ;
    lv2:symbol "out1" ;
    lv2:name "Out1"
  ]  , [
    a lv2:AudioPort ;
    a lv2:OutputPort ;
    lv2:index 2 ;
    lv2:symbol "out2" ;
    lv2:name "Out2"
  ] , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 3 ;
    lv2:symbol "left_delay" ;
    lv2:name "Constant delay (left, samples)";
    lv2:default 32 ;
    lv2:minimum 0 ;
    lv2:maximum 1024 ;
  ]  , [
    a lv2:InputPort ;
    a lv2:ControlPort ;
    lv2:index 4 ;
    lv2:symbol "maxdelay" ;
    lv2:name "Maximum delay (right, samples)";
    lv2:default 64 ;
    lv2:minimum 0 ;
    lv2:maximum 1024 ;
  ] .
